{
  "profiles": {
    "StudentCredits.http": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "PROCESS_TYPE": "local"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5247"
    },
    "StudentCredits.https": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "PROCESS_TYPE": "local"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7092;http://localhost:5247"
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "PROCESS_TYPE": "iis-express"
      }
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://+:519;http://+:403",
        "ASPNETCORE_HTTPS_PORTS": "23019",
        "ASPNETCORE_HTTP_PORTS": "22163",
        "PROCESS_TYPE": "docker"
      },
      "publishAllPorts": true,
      "useSSL": true
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:2246",
      "sslPort": 44379
    }
  }
}
